{
  "name": "mathschuh/sftp-tracker",
  "description": "Track files in a sftp folder.",
  "keywords": ["sftp", "tracker"],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "phpseclib/phpseclib": "^3.0.19",
    "monolog/monolog": "3.3.1"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "8.2"
    },
    "sort-packages": true
  }
}